SELECT capitulos.id, capitulos."Video", capitulos_temp."Video"
FROM capitulos_temp JOIN capitulos
ON (capitulos_temp.id = capitulos.id)
WHERE capitulos."Serie_id" = '5'
AND capitulos.id >= '318'
ORDER BY capitulos.id;

UPDATE capitulos
SET "Video" = capitulos_temp."Video"
FROM capitulos_temp
WHERE capitulos.id = capitulos_temp.id
AND capitulos."Serie_id" = '5'
AND capitulos.id = '318';


