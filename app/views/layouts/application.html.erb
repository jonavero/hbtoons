<body data-turbolinks="false">

<header class="headerTitulo parallax">
  <h1 class="text-center"> <%= link_to 'LACARTOONS', inicio_path, class: 'hbtoonh1' %></h1>
  <span class="icono-menu fa fa-bars"></span>
</header>

<nav class="menu">
  <div class="">

    <ul class="navegacion">
      <li><span class="fa fa-home"><%= link_to 'Inicio', inicio_path %></span></li>
      <li><%= link_to 'Categorias', inicio_path %></li>
      <li><%= link_to 'Contactos', contacto_path %></li>
    </ul>


    <%= form_tag inicio_path, method: :get, :class => 'formulario-buscar' do %>
      <%= text_field_tag :Titulo, params[:Titulo], :class => 'inputS', :placeholder => 'Buscar tu serie' %>
      <%= submit_tag '', :class => 'fa fa-search noBorden' %>
      <button class="fa fa-search" type="seach" name="button"></button>
    <% end %>
  </div>
</nav>




<%= yield %>

<footer class="footer-pagina">
  <div class="container">
    <div class="donacion">
      <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_blank">
